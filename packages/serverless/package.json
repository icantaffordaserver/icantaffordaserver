    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "js-yaml": "^3.10.0",
    "lint-staged": "^5.0.0",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.1",
