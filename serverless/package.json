{
  "name": "serverless",
  "version": "0.0.1",
  "files": ["bin"],
  "description": "The serverless functions powering the backend of toktumi",
  "main": "index.js",
  "scripts": {
    "start:dev":
      "concurrently --kill-others 'serverless offline -P 4000 --stage dev' 'ngrok http -subdomain=toktumi-api 4000'",
    "prepareLibs": "node scripts/prepareLibs.js",
    "build": "node scripts/bundleLambdas.js",
    "cmp": "babel functions/sampleExpress/src -d functions/sampleExpress/lib",
    "create":
      "npm run transpile && claudia create --region us-east-1 --api-module bin/claudia",
    "update": "npm run transpile && claudia update",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Alexander Mann",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "concurrently": "^3.5.0",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "js-yaml": "^3.10.0",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.1",
    "serverless": "^1.12.0",
    "serverless-offline": "^3.15.3",
    "serverless-plugin-webpack": "^1.0.0",
    "serverless-webpack": "^3.0.0",
    "shelljs": "^0.7.7",
    "webpack": "^2.4.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "axios": "^0.16.2",
    "babel-eslint": "^7.2.3",
    "babel-preset-babili": "^0.0.12",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "graphql-request": "^1.3.6",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "postmark": "^1.4.1",
    "source-map-support": "^0.4.15",
    "validator": "^8.2.0"
  }
}
